using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication17
{
    class Program
    {
        
            static void Main(string[] args)
            {
                Console.WriteLine("Even numbers from 1 to 20 (excluding 10):");

                for (int i = 1; i <= 20; i++)
                {
                    if (i % 2 == 0 && i != 10)
                    {
                        Console.WriteLine(i);
                    Console.ReadLine();
                    }
                }
            }
        }
    }
