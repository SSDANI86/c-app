using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication17
{
    class Program
    
        {
            static void Main()
            {
                for (int i = 1; i <= 20; i++)
                {
                    if (i == 10)
                    {
                        continue; // اگر عدد 10 بود، آن را نادیده می‌گیرد
                    }
                    Console.WriteLine(i); // چاپ اعداد
                Console.ReadLine();
                }
            }
        }
    }
